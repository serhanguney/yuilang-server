{
    "version": "2.0.2",
    "compilerOptions": {
      "outDir": "./build",
      "lib": ["ES5","ES6"],
      "target": "es2017",
      "moduleResolution": "node",
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "skipLibCheck": true
    },
  "exclude": [
    "node_modules"
  ],
  "include": ["./src/**/*"]
}
